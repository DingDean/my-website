///* Palette generated by Material Palette - materialpalette.com/blue-grey/teal */
dark-primary-c    = #616161
default-primary-c = #607D8B
light-primary-c   = #F5F5F5
text-primary-c    = #FFFFFF
accent-c          = #009688
primary-text-c    = #212121
secondary-text-c  = #757575
divider-c         = #707070
hover-c           = #01579B


html, body, h1, h2, h3, h4, h5, h6, p, ul
  padding 0
  margin 0
ul
  list-style-type none
a
  text-decoration none

body, html
  font-family -apple-system, BlinkMacSystemFont, "Segoe UI",
              Roboto, "Helvetica Neue", Helvetica, "PingFang SC",
              "Hiragino Sans GB", "Microsoft YaHei", SimSun, sans-serif
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale
  p
    font-size 14px
    line-height 22px

